{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "dist",
		"lib": [
			"ESNext"
		],
		"sourceMap": true,
		"rootDir": "edge",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}